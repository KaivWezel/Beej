<!DOCTYPE html>
<html lang="en">
	<head>
		<%- include('./partials/head.ejs') %>
	</head>
	<body>
		<h1>Login</h1>
		<main>
			<form action="/login" method="post">
				<div class="input-field">
					<label for="email">E-mail</label>
					<input
						type="email"
						id="email"
						name="email"
						autocomplete="email"
						required
						autofocus
					/>
				</div>
				<div class="input-field">
					<label for="current-password">Password</label>
					<input
						type="password"
						id="current-password"
						autocomplete="current-password"
						name="password"
					/>
				</div>
				<button>Login</button>
			</form>
		</main>
	</body>
</html>
